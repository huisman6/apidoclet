<?xml version="1.0" encoding="UTF-8"?>
<!-- https://svn.apache.org/repos/asf/maven/plugins/trunk/maven-scm-publish-plugin/pom.xml -->
<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>apidoc</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<apidoc>
						com.dooioo.se.apidoclet:apidoclet-maven-plugin:${project.version}:deploy
					</apidoc>
				</phases>
			</configuration>
		</component>
		<!-- 自定义生命周期 -->
		<component>
			<role>org.apache.maven.lifecycle.Lifecycle</role>
			<implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
			<role-hint>apidoc</role-hint>
			<configuration>
				<id>apidoc</id>
				<phases>
					<phase>apidoc</phase>
				</phases>
				<default-phases>
					<apidoc>
						com.dooioo.se.apidoclet:apidoclet-maven-plugin:deploy
					</apidoc>
				</default-phases>
			</configuration>
		</component>
	</components>
</component-set>
